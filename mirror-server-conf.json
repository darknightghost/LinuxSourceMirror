			"address" : "0.0.0.0",
			"port" : 8000
		}
	},
	"distros" : {
		"archlinux" : {
			"url" : "rsync://rsync.mirrors.ustc.edu.cn/repo/archlinux/",
			"servers" : ["http"]
		},
		"archlinuxcn" : {
			"url" : "rsync://rsync.mirrors.ustc.edu.cn/repo/archlinuxcn/",
			"servers" : ["http"]
		}
	}
